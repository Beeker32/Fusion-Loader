//Custom Load Screen *Fetch The Script*
javascript:(function() {
    var scriptUrl = 'https://raw.githubusercontent.com/PXIFusionX/PXI-Fusion-X/main/Custom-Load-Screen/Load%20Screen%20Code';
    fetch(scriptUrl)
        .then(response => response.text())
        .then(scriptContent => {
            var script = document.createElement('script');
            script.textContent = scriptContent;
            document.body.appendChild(script);
        })
        .catch(error => console.error('Error fetching the script:', error));
        
        var scriptUrl = 'https://raw.githubusercontent.com/PXIFusionX/Fusion-Loader/main/Hacks/Selector%20(Advanced)';
        fetch(scriptUrl)
            .then(response => response.text())
            .then(scriptContent => {
                var script = document.createElement('script');
                script.textContent = scriptContent;
                document.body.appendChild(script);
            })
            .catch(error => console.error('Error fetching the script:', error));})();

//Load Cheat Menu *Fetch The Script*
javascript:(function() {
    var scriptUrl = 'https://raw.githubusercontent.com/PXIFusionX/PXI-Fusion-X/main/Cheat-Menu/Menu';
    fetch(scriptUrl)
        .then(response => response.text())
        .then(scriptContent => {
            var script = document.createElement('script');
            script.textContent = scriptContent;
            document.body.appendChild(script);
        })
        .catch(error => console.error('Error fetching the script:', error));
})();

